{
    "containerDefinitions": [
        {
            "name": "migration-runner",
            "image": "placeholder",
            "cpu": 256,
            "memory": 512,
            "portMappings": [],
            "essential": true,
            "command": [
                "timeout",
                "3600",
                "alembic",
                "upgrade",
                "head"
            ],
            "environment": [
                {
                    "name": "MYSQL_USERNAME",
                    "value": "${MYSQL_USERNAME}"
                  },
                  {
                    "name": "MYSQL_PASSWORD",
                    "value": "${MYSQL_PASSWORD}"
                  },
                  {
                    "name": "MYSQL_HOST",
                    "value": "${MYSQL_HOST}"
                  },
                  {
                    "name": "MYSQL_PORT",
                    "value": "${MYSQL_PORT}"
                  }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/migration-task",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "migration"
                }
            },
            "systemControls": []
        }
    ],
    "family": "migration-task",
    "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ecs-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "512"
}